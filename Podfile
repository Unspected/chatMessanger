# platform :ios, '9.0'

target 'Messenger' do
use_frameworks!

# Firebase
pod 'Firebase/Auth'
pod 'Firebase/Core'
pod 'Firebase/Database'

# Facebook
pod 'FBSDKCoreKit'
pod 'FBSDKLoginKit'
pod 'FBSDKShareKit'

# Google Sign In
pod 'GoogleSignIn'

#Pod dependence 
pod 'MessageKit'
pod 'JGProgressHUD'
pod 'SDWebImage'
    
end
